    public void ctrlMM() // no need to create it into the dialogContent area
        {
            ctrlMMactions(); // I called it directly from parent to the steps.
        }

    public void ctrlMMactions()
        {
            Actions actions = new Actions(GWD.getDriver());
            actions.keyDown(Keys.CONTROL).sendKeys("m").sendKeys("m").keyUp(Keys.CONTROL).perform
        }


