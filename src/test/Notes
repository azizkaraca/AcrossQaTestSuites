    @And("User chooses a Breadcrumb and verify current Url")
       public void userChoosesABreadcrumbAndVerifyCurrentUrl(DataTable elements) {

           List<List<String>> listElement=elements.asLists(String.class);

           for (int i = 0; i < listElement.size(); i++) {
               bc.findAndClick(listElement.get(i).get(0));  // first element
               bc.verifyCurrentUrl(listElement.get(i).get(1)); // second element
           }
       }